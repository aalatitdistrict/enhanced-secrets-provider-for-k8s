#!/bin/bash -e

DOCKERHUB_IMAGE='cyberark/cyberark-secrets-provider-for-k8s'
VERSION_TAG="$(<VERSION)"

docker tag cyberark-secrets-provider-for-k8s:dev "$DOCKERHUB_IMAGE"
docker tag cyberark-secrets-provider-for-k8s:dev "$DOCKERHUB_IMAGE:$VERSION_TAG"

docker push $DOCKERHUB_IMAGE
docker push "$DOCKERHUB_IMAGE:$VERSION_TAG"
